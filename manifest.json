{
  "name": "History Autowipe",
  "description": "Automatically clears browsing history, cookies, or all data at custom intervals.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "alarms"
  ],
  "optional_permissions": [
    "browsingData"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
